;; test_parallel_fold.lisp - Tests for fold operations

;; Define left fold
(define foldl [f] [acc] [lst]
  (match lst
    ()       acc
    (h .. t) (foldl f (f acc h) t)))

;; TEST: foldl sum
;; EXPECT: 6
(foldl (lambda [acc] [x] (+ acc x)) 0 '(1 2 3))

;; TEST: foldl on empty
;; EXPECT: 42
(foldl (lambda [acc] [x] (+ acc x)) 42 nil)

;; EXPECT: 6
(foldl (lambda [acc] [x] (* acc x)) 1 '(1 2 3))
