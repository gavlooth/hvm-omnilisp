;; test_dict_literal.lisp - Tests for dictionary literals

;; Empty dict
;; TEST: empty dict
;; EXPECT: #{}
#{}

;; Dict with symbol keys
;; TEST: dict with colon-quoted keys
;; EXPECT: #{:a 1 :b 2 :c 3}
#{:a 1 :b 2 :c 3}

;; Dict with string keys
;; TEST: dict with string keys
;; EXPECT: #{"name" "Alice" "age" 30}
#{"name" "Alice" "age" 30}

;; Dict with integer keys
;; TEST: dict with integer keys
;; EXPECT: #{1 "one" 2 "two" 3 "three"}
#{1 "one" 2 "two" 3 "three"}

;; Nested dicts
;; TEST: nested dict
;; EXPECT: #{:person #{:name "Bob" :age 25}}
#{:person #{:name "Bob" :age 25}}

;; Dict with mixed value types
;; TEST: dict with mixed values
;; EXPECT: #{:num 42 :str "hello" :lst (1 2 3)}
#{:num 42 :str "hello" :lst '(1 2 3)}
