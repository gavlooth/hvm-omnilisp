;; test_math_sqrt_pow.lisp - Tests for sqrt and pow functions

;; Square root
;; TEST: sqrt of perfect square
;; EXPECT: 3
(sqrt 9)

;; TEST: sqrt of 16
;; EXPECT: 4
(sqrt 16)

;; TEST: sqrt of 1
;; EXPECT: 1
(sqrt 1)

;; TEST: sqrt of 0
;; EXPECT: 0
(sqrt 0)

;; Power function
;; TEST: 2^3 = 8
;; EXPECT: 8
(pow 2 3)

;; TEST: 3^2 = 9
;; EXPECT: 9
(pow 3 2)

;; TEST: anything^0 = 1
;; EXPECT: 1
(pow 5 0)

;; TEST: anything^1 = itself
;; EXPECT: 7
(pow 7 1)

;; TEST: 2^10 = 1024
;; EXPECT: 1024
(pow 2 10)

;; Combined: verify sqrt(pow(x,2)) = x
;; TEST: sqrt of square
;; EXPECT: 5
(sqrt (pow 5 2))
