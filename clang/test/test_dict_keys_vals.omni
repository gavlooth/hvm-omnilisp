;; test_dict_keys_vals.lisp - Tests for dict-keys, dict-vals, dict-entries

;; Get all keys
;; TEST: dict-keys
;; EXPECT: (:a :b :c)
(dict-keys #{:a 1 :b 2 :c 3})

;; TEST: dict-keys empty
;; EXPECT: ()
(dict-keys #{})

;; Get all values
;; TEST: dict-vals
;; EXPECT: (1 2 3)
(dict-vals #{:a 1 :b 2 :c 3})

;; TEST: dict-vals empty
;; EXPECT: ()
(dict-vals #{})

;; Get entries as key-value pairs
;; TEST: dict-entries
;; EXPECT: ((:a 1) (:b 2) (:c 3))
(dict-entries #{:a 1 :b 2 :c 3})

;; TEST: dict-entries empty
;; EXPECT: ()
(dict-entries #{})

;; Count entries
;; TEST: dict-count
;; EXPECT: 3
(dict-count #{:a 1 :b 2 :c 3})

;; TEST: dict-count empty
;; EXPECT: 0
(dict-count #{})

;; Iterate over dict
(define dict-sum-vals [d]
  (foldl (lambda [acc] [v] (+ acc v)) 0 (dict-vals d)))

;; TEST: sum all values in dict
;; EXPECT: 6
(dict-sum-vals #{:a 1 :b 2 :c 3})
